{
  "ApplicationInsights": {
    "InstrumentationKey": "{instrumentationKey}"
  },
  "Certes": {
    "DirectoryUri": "https://acme-v01.api.letsencrypt.org/directory",
    "MaxSanPerCert": "100",
    "RenewBeforeExpire": "30",
    "Azure": {
      "ServicePrincipal": {
        "TenantId": "{tenantId}",
        "ClientId": "{clientId}",
        "ClientSecret": "{clientSecret}"
      },
      "WebApp": {
        "SubscriptionId": "{subscriptionId}",
        "ResourceGroup": "{resourceGroup}",
        "Name": "{name}",
        "Slot": "{slot}"
      }
    }
  }
}