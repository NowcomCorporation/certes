{
  "version": "1.0.0-*",

  "dependencies": {
    "Certes": "1.0.0-*",
    "Certes.Integration": "1.0.0-*",
    "Microsoft.Azure.Management.Dns": "1.7.2-preview",
    "Microsoft.Azure.Management.ResourceManager": "1.1.3-preview",
    "Microsoft.Azure.Management.Websites": "1.3.2-preview",
    "Microsoft.IdentityModel.Clients.ActiveDirectory": "3.12.0",
    "WindowsAzure.Storage": "7.1.3-preview"
  },

  "frameworks": {
    "netstandard1.6": {
      "imports": [
        "dnxcore50",
        "portable-net451+win8"
      ]
    },
    "net46": {
      "frameworkAssemblies": {
        "System.Linq": ""
      }
    }
  },
  "buildOptions": {
    "embed": [
      "Resources/**/*.*"
    ],
    "warningsAsErrors": true,
    "nowarn": [ "CS1591" ],
    "xmlDoc": true
  }
}
